{
    "name": "atog/phpokemon",
    "type": "library",
    "description": "atog/phpokemon is a PHP 5.5+ wrapper for PokeAPI",
    "keywords": [
        "atog",
        "phpokemon",
        "pokeapi"
    ],
    "homepage": "https://github.com/atog/phpokemon",
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Kleindienst",
            "email": "pascal.kleindienst@posteo.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "atog\\phpokemon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "atog\\phpokemon\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
